  "classification_rules": {
    "description": "Regels voor het classificeren van Office versies op basis van build number en leeftijd",
    "rules": [
      {
        "name": "Current Channel - Nieuwste",
        "condition": "build >= 19426",
        "channel": "Current Channel",
        "status": "Actueel",
        "color": "#28a745",
        "bold": true,
        "priority": 1
      },
      {
        "name": "Current Channel - Recent (< 30 dagen oud)",
        "condition": "build >= 19328 AND build < 19426 AND age_days <= 30",
        "channel": "Current Channel (recent)",
        "status": "Recent",
        "color": "#28a745",
        "bold": false,
        "priority": 2
      },
      {
        "name": "Current Channel - Verouderd (> 30 dagen oud)",
        "condition": "build >= 19328 AND build < 19426 AND age_days > 30",
        "channel": "Current Channel (verouderd)",
        "status": "Verouderd - update aanbevolen",
        "color": "#ffc107",
        "bold": false,
        "priority": 3
      },
      {
        "name": "Monthly Enterprise - Nieuwste",
        "condition": "build == 19328",
        "channel": "Monthly Enterprise",
        "status": "Actueel",
        "color": "#28a745",
        "bold": false,
        "priority": 4
      },
      {
        "name": "Monthly Enterprise - Ondersteund",
        "condition": "build >= 19127 AND build < 19328",
        "channel": "Monthly Enterprise",
        "status": "Ondersteund",
        "color": "#28a745",
        "bold": false,
        "priority": 5
      },
      {
        "name": "Semi-Annual Enterprise",
        "condition": "build >= 17928 AND build < 19127",
        "channel": "Semi-Annual Enterprise",
        "status": "Ondersteund",
        "color": "#ffc107",
        "bold": false,
        "priority": 6
      },
      {
        "name": "Zeer verouderd / EOL",
        "condition": "build < 17928",
        "channel": "Onbekend/Verouderd",
